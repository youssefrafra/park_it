<div class="container">
        <div class="card mb-3">
            <div style="width:100%">
            <%= image_tag "parking-spot", alttext:"", class:"card-img-top" %>
            </div>
            <div class="card-body">
                <h5 class="card-title">Offer title</h5>
                <p class="card-text"><%= @parking.description%></p>
                <p class="card-text"><small class="text-muted">Last updated <%= time_ago_in_words(@parking.created_at)%></small></p>
            </div>
        </div>
</div>